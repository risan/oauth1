{
    "name": "risan/oauth1",
    "type": "library",
    "description": "OAuth 1.0 client library for PHP",
    "keywords": [
        "oauth",
        "oauth1",
        "authorization",
        "authentication",
        "twitter",
        "tumblr",
        "upwork"
    ],
    "homepage": "https://github.com/risan/oauth1",
    "license": "MIT",
    "authors": [
        {
            "name": "Risan Bagja Pradana",
            "email": "risanbagja@gmail.com",
            "homepage": "https://risan.io",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Risan\\OAuth1\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Risan\\OAuth1\\Test\\": "tests"
        }
    },
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.3",
        "guzzlehttp/psr7": "^1.4.2",
        "paragonie/random_compat": "~1.4|~2.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7|~6.4"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
